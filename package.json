{
  "name": "express-session",
  "version": "1.0.0",
  "main": "./dist/main.js",
  "scripts": {
    "build": "webpack --config webpack.prod.config.js",
    "build:dev": "webpack --config webpack.dev.config.js",
    "start": "npm run build && node . | npx winston",
    "server": "node . | npx winston",
    "dev": "npx concurrently \"npm run watch:build\" \"npm run watch:server\"",
    "watch:server": "nodemon \"./dist/main.js\" --watch \"./dist\" | npx winston",
    "watch:build": "webpack -w --config webpack.dev.config.js",
    "full": "export PORT=4000 && npm run dev"
  },
